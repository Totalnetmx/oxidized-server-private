---
username: oxidized
password: __CHANGE_ME__
interval: 3600
use_syslog: false
log: /home/oxidized/logs/oxidized.log
debug: false
threads: 30
timeout: 20
retries: 2
prompt: !ruby/regexp /^([\w.@-]+[#>]\s?)$/
rest: 0.0.0.0:8888

input:
  default: ssh
  debug: false
  ssh:
    secure: false

output:
  default: file
  file:
    directory: /home/oxidized/repos

source:
  default: csv
  csv:
    file: /home/oxidized/router.db
    delimiter: !ruby/regexp /:/
    map:
      name: 0
      ip: 1
      model: 2
      group: 3
    vars_map:
      ssh_port: 4

model_map:
  mikrotik: routeros
  mikrotik-v7: routeros
  arista: eos
  cisco: ios
